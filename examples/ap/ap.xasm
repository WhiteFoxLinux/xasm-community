--等差数列 XASM 实现
sto
0x00 --当前项 首项
0x10 --公差
0x00 --当前数列的长度
0xfe --最终数列的长度
0x4d --循环结束位置
"\n" --分隔符
sto
mov 0x00 0x00 0xf0 0x00  0x00 0x00 0x00 0x01
mov 0x00 0x00 0xf0 0x02  0x00 0x00 0x00 0x06
geta 0x00 0x00 0xf0 0x01
add 0x00 0x00 0x00 0x03  0x00 0x00 0xf0 0x00
putn 0x00 0x00 0xf0 0x00
putc 0x00 0x00 0xf0 0x02
add 0x00 0x00 0x00 0x01  0x00 0x00 0x00 0x02   
=  0x00 0x00 0x00 0x03  0x00 0x00 0x00 0x04  0x00 0x00 0x00 0x05
goto 0x00 0x00 0xf0 0x01
exit

